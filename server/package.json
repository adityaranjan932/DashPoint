{
  "name": "dashboard-server",
  "version": "1.0.0",
  "description": "Backend server for MERN Dashboard application",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "nodemon src/server.js",
    "prod": "NODE_ENV=production node src/server.js",
    "start-with-agent": "powershell -ExecutionPolicy Bypass -File scripts/start-dashpoint-agent.ps1 && npm start",
    "dev-with-agent": "powershell -ExecutionPolicy Bypass -File scripts/start-dashpoint-agent.ps1 && npm run dev",
    "start-agent": "powershell -ExecutionPolicy Bypass -File scripts/start-dashpoint-agent.ps1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "dashboard",
    "mern",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10"
  }
}
